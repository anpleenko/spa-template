    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.0",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "eslint": "9.9.1",
    "eslint-kit": "10.31.0",
    "imagemin-lint-staged": "0.5.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "nano-staged": "0.8.0",
    "postcss": "8.4.33",
    "prettier": "3.2.5",
    "sort-package-json": "2.10.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "29.1.3",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-tsconfig-paths": "4.3.1"
