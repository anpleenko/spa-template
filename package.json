{
  "name": "spa-template",
  "version": "0.0.1",
  "description": "Template for SPA projects",
  "keywords": [
    "react",
    "SPA",
    "template",
    "Typescript"
  ],
  "homepage": "https://github.com/anpleenko/spa-template#readme",
  "bugs": {
    "url": "https://github.com/anpleenko/spa-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpleenko/spa-template.git"
  },
  "license": "MIT",
  "author": "Alexander Anpleenko <avanpleenko@gmail.com>",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "postinstall": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "start": "vite",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "validate": "./scripts/validate"
  },
  "resolutions": {
    "eslint-import-resolver-typescript": "3.6.1"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@testing-library/jest-dom": "6.6.3",
    "@types/jest": "29.5.14",
    "@types/node": "22.16.5",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.7.0",
    "autoprefixer": "10.4.21",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-kit": "11.39.0",
    "imagemin-lint-staged": "0.5.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "nano-staged": "0.8.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "sort-package-json": "3.4.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4"
  },
  "packageManager": "yarn@4.9.1"
}
